#include <bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
 
#define testcase() {int t; cin >> t; while(t--) {solve();}}
#define all(x) (x).begin(), (x).end()
#define sz(x) ((int)(x).size())
#define pb push_back
#define bug() cout << "######PLEASE_NO_BUG######\n"
const int nxM = 500 + 7;
string n, m;
int dp[nxM][nxM];
 
void solve() {
	int a, b;
	cin >> a >> b;
	for(int i = 1; i <= a; i++) {
		for(int j = 1; j <= b; j++) {
			if(i ^ j) {
				dp[i][j] = 1e9 + 7;
				for(int k = 1; k <= i; k++) {
					dp[i][j] = min(dp[i][j], dp[k][j] + dp[i - k][j] + 1);
				}
				for(int k = 1; k <= j; k++) {
					dp[i][j] = min(dp[i][j], dp[i][k] + dp[i][j - k] + 1);
				}
			}
		}
	}
	cout << dp[a][b] << "\n";
}
 
int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(nullptr);
	solve();
	return 0;
}
