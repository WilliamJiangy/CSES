// @Author: WilliamJiangy
#include <bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
 
#define testcase() {int t; cin >> t; while(t--) {solve();}}
#define all(x) (x).begin(), (x).end()
#define sz(x) ((int)(x).size())
#define pb push_back
#define bug() cout << "######PLEASE_NO_BUG######\n"
const int nxM = 1e5 + 7, nxX = 107;
int n, x[nxX]; 
bool dp[nxM];
 
void solve() {
	cin >> n;
	for(int i = 0; i < n; i++) cin >> x[i];
	dp[0] = 1;
	for(int i = 0; i < n; i++) {
		for(int j = nxM - 7; j; j--) {
			if(x[i] <= j) {
				dp[j] |= dp[j - x[i]];
			}
		}
	}
	vector<int> ans;
	for(int i = 1; i <= nxM - 7; i++) {
		if(dp[i]) {
			ans.pb(i);
		}
	}
	cout << sz(ans) << "\n";
	for(int a : ans) cout << a << " ";
}
 
int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(nullptr);
	solve();
	return 0;
}
